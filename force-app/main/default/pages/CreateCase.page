<apex:page standardController="Case" showHeader="false" sidebar="false">
    <apex:stylesheet value="https://fonts.googleapis.com/css2?family=Righteous&display=swap"/>
    <apex:Pagemessages id="msg"/>
    <apex:include pageName="Header"/>
    
    <apex:form >
    <apex:pageBlock title="Create Case">
        <apex:pageMessages />
        
        <apex:pageBlockSection columns="1">
    <apex:inputField value="{! Case.subject }"/>
    <apex:inputField value="{! Case.Description }"/>
    <apex:inputField value="{! Case.SuppliedName }"/>
    <apex:inputField value="{! Case.SuppliedEmail }"/>
    <apex:inputField value="{! Case.SuppliedPhone }"/>
    <apex:inputField value="{! Case.SuppliedCompany }"/>
    
</apex:pageBlockSection>
        <apex:pageBlockButtons >
            <apex:commandButton action="{! save }" value="Save" />
        </apex:pageBlockButtons>
    </apex:pageBlock>
        

    </apex:form>
    <apex:include pageName="Footer"/>
    <style>
      font-family: 'Righteous', cursive;
    </style>
</apex:page>