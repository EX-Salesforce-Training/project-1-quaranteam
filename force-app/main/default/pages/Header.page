<apex:image value="http://rebc.biz/dist/img/header_banner.jpg" width="3%"

height="3%"/>

</div>

<br/>

<div class="name" align="right" bgcolor="#993333"> 

<apex:outputText value="{!$User.FirstName}"/>

<br/>

<apex:outputText value="{!$User.Email}"/>

</div>

<apex:form >
